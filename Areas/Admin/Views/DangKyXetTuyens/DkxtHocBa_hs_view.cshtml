    <div class="card card-collapsable">
        <div class="card-header mt-0 mb-0">
            <span>Kiểm duyệt hồ sơ</span>
        </div>
        <div class="card-body">
            <div class="col-lg-12 col-12">
                <div class="row ">
                    <div class="col-xxl-7 col-lg-12">
                        <input class="form-control" type="text" id="DuTuyen_ThongBaoKiemDuyet" name="DuTuyen_ThongBaoKiemDuyet" placeholder="Nhập thông báo (nếu có)">
