    function HocVienDuTuyenGetByID() {
        var data_obj = { DuTuyen_ID: $('#hiden_dutuyen_id_present').val() }; /*alert(DuTuyen_ID);   */
        console.log(data_obj);
        $.ajax({
            url: "/HocVienDangKys/DsHvDuTuyenKiemTraHoSoJson/",
            type: "POST",
            data: JSON.stringify(data_obj),
            contentType: 'application/json;charset=utf-8',
            async: false,
            success: function (res) {
                console.log(res.success);
                if (res.success) {
                    var data = res.data;
                    console.log(data);

                    if (data.DuTuyen_TrangThai == 1) {
                        $('#radioDung').removeAttr('checked', 'checked');
                        $('#radioSai').removeAttr('checked', 'checked');
                    }
                    else {
                        if (data.DuTuyen_TrangThai == 9) {
                            $('#radioHoSoDayDu').attr('checked', 'checked');
                            $('#radioHoSoThieu').removeAttr('checked', 'checked');
                            $('#DuTuyen_ThongBaoKiemDuyet').val(data.DuTuyen_ThongBaoKiemDuyet);
                        }
                        else if (data.DuTuyen_TrangThai == 2) {
                            $('#radioHoSoDayDu').removeAttr('checked', 'checked');
                            $('#radioHoSoThieu').attr('checked', 'checked');
                            $('#DuTuyen_ThongBaoKiemDuyet').val(data.DuTuyen_ThongBaoKiemDuyet);
                        }
                    }

                    $('#HocVien_HoDem').val(data.HocVien_HoDem);
                    $('#HocVien_Ten').val(data.HocVien_Ten);

                    if (data.HocVien_GioiTinh == 1) {
                        $('#radioNam').attr('checked', 'checked');
                        $('#radioNu').attr('disabled', 'disabled');
                    } else if (data.HocVien_GioiTinh == 0) {
                        $('#radioNu').attr('checked', 'checked');
                        $('#radioNam').attr('disabled', 'disabled');
                    }
                    $('#HocVien_DanToc').val(data.HocVien_DanToc);
                    $('#HocVien_NgaySinh').val(data.HocVien_NgaySinh);
                    $('#HocVien_CCCD').val(data.HocVien_CCCD);
                    $('#HocVien_CCCD_NgayCap').val(data.HocVien_CCCD_NgayCap);
                    $('#HocVien_DienThoai').val(data.HocVien_DienThoai);
                    $('#HocVien_Email').val(data.HocVien_Email);

                    $('#HocVien_NoiSinh').val(data.HocVien_NoiSinh);
                    $('#HocVien_HoKhauThuongTru').val(data.HocVien_HoKhauThuongTru);
                    $('#HocVien_NoiOHienNay').val(data.HocVien_NoiOHienNay);
                    $('#HocVien_DiaChiLienHe').val(data.HocVien_DiaChiLienHe);

