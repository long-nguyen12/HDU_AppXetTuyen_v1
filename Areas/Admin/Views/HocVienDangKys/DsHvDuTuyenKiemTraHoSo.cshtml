                    }
                    else {
                        var str_a = `<a >Không có dữ liệu</a> &ensp;`;
                        $('#div_list_view_mckhac').append(str_a);
                    }

                    $("#hiden_HocVien_LePhi_TrangThai").val(data.HocVien_LePhi_TrangThai);

                    if (data.HocVien_LePhi_TrangThai == 0) {
                        /* $('#div_thongtin_lephi').css("display", "none");*/
                        $('#div_dung').hide();
                        $('#div_sai').hide();
                        $('#stc_number').text("Chưa nộp lệ phí");
                        $('#stc_txt').text('');
                    }
                    else if (data.HocVien_LePhi_TrangThai > 0) {
