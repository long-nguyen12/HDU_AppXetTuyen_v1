                        }
                        if (data.HocVien_LePhi_TepMinhChung) {
                            const ArrTepMinhChungLePhi = data.HocVien_LePhi_TepMinhChung.split("#");
                            for (i = 0; i < ArrTepMinhChungLePhi.length - 1; i++) {
