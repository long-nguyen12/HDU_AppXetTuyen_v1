                        }
                        if (data.HocVien_LePhi_TrangThai == 2) {
                            $('#radioDung').removeAttr('checked', 'checked');
                            $('#radioSai').attr('checked', 'checked');
                        }

                        if (data.HocVien_LePhi_TrangThai == 9) {
                            $('#radioDung').attr('checked', 'checked');
