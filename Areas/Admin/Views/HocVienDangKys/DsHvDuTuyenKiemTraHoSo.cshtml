                        $('#div_list_view_cc_bang_ngoaingu').append(str_a);
                    }


                    if (data.HocVien_MCKhac) {
                        const ArrMinhChungKhac = data.HocVien_MCKhac.split("#");
                        for (i = 0; i < ArrMinhChungKhac.length - 1; i++) {
