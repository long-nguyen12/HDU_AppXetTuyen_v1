<script>

    let key_dkxt_id_port = -1;

    class _thongtin_monhoc {
        constructor(ten_Mon, H_K1, H_K2, H_K3, DiemTrung_Binh) {
            this.ten_Mon = ten_Mon;
            this.H_K1 = H_K1;
            this.H_K2 = H_K2;
            this.H_K3 = H_K3;
            this.DiemTrung_Binh = DiemTrung_Binh;
        }
    }

    class _thongtin_bosung_uutien {
        constructor(ut_doituong_ten, ut_doituong_diem_ut, _xlhocluc_12, xl_hankiem_12) {
            this.ut_doituong_ten = ut_doituong_ten;
            this.ut_doituong_diem_ut = ut_doituong_diem_ut;

            this.ut_khuvuc_ten = ut_khuvuc_ten;
            this.ut_khuvuc_diem_ut = ut_khuvuc_diem_ut;

            this._xlhocluc_12 = _xlhocluc_12;
            this.xl_hankiem_12 = xl_hankiem_12;
        }
    }
    //{"TenMon1":"Toán","HK1":"8.00","HK2":"8.00","HK3":"8.00","DiemTrungBinh":"8.00"}
    $(document).ready(function () {

        dkxt_get_data_da_dkxt_all();
        // get_data_forselect_khoinganh_all(0, 0, 0);

        $("#select_khoinganh").change(function () {
            get_data_forselect_nganh_all($("#select_khoinganh").val(), 0, 0);
        })
        $("#select_nganh").change(function () {
            get_data_forselect_tohopmon_all($("#select_nganh").val(), 0)
        })
        // lấy danh sách 3 môn cần nhập điểm
        $("#select_tohopmon").change(function () {
            // get_data_fortable_nhapdiem($("#select_tohopmon").val());
            get_data_fortable_nhapdiem();
        })
    });

    function dkxt_get_data_da_dkxt_all() {
        let _output = "";
        $.ajax({
            url: "/NCDangKyXetTuyens/dkxthocbaListAll/",
            type: 'GET',
            contentType: 'application/json;charset=utf-8',
            dataType: 'json',
            success: function (result_all) {
                // console.log(result_all)
                if (result_all.length == 0) {
                    _output += '<tr><td colspan="6"  style="text-align:center">Chưa đăng ký nguyện vọng nào!</td></tr>';
                }
                else {
                    $.each(result_all, function (i, result) {

                        _output += '<tr>' +
                            '<td style="vertical-align: middle !important "> NV ' + result.Dkxt_NguyenVong + ' <a class="btn btn-light btn-sm">&#8743;</a>  <a class="btn btn-light btn-sm">&#8744;</a> </td>' +
                            '<td style="vertical-align: middle !important "> ' + result.Dkxt_TenAll.Nganh_GhiChu + ' </td>' +
                            '<td style="vertical-align: middle !important ">' + result.Dkxt_TenAll.Thm_MaTen + '</td>' +
                            '<td style="vertical-align: middle !important ">' + result.Dkxt_Diem_Tong_Full + '</td>' +
                            '<td>' + '<a href="#" class="btn btn-light" style="vertical-align: middle !important "> <input type="checkbox" checked disabled /> Đã xác thực đăng ký</a>' + '</td>' +
                            '<td style="text-align: center; vertical-align: middle !important ">' +
                            'Sửa | Xóa' +
                            '</td>' +
                            '</tr>'
                    });
                }
                document.getElementById('tbody_list_nguyenvong').innerHTML = _output;
            },
            error: function (errormessage) {
                alert(errormessage.responseText);
            }
        });
    }
    function dkxt_add_data_show_div_add(khoinganh_key, nganh_key, tohopmon_key) {
        document.getElementById('div_list_nguyenvong').style.display = "none";
        document.getElementById('div_update_dk_nguyenvong').style.display = "inline";
        get_data_forselect_khoinganh_all(khoinganh_key, nganh_key, tohopmon_key);
        key_dkxt_id_port = -1;
    }
    function onclick_bt_huybo_dangky_nvmoi(khoinganh_key, nganh_key, tohopmon_key) {

        document.getElementById('div_list_nguyenvong').style.display = "inline";
        document.getElementById('div_update_dk_nguyenvong').style.display = "none";
        get_data_forselect_khoinganh_all(khoinganh_key, nganh_key, tohopmon_key);
        key_dkxt_id_port = -2;
    }

    function get_data_forselect_khoinganh_all(khoinganh_key, nganh_key, tohopmon_key) {

        $('#select_khoinganh').empty()
        $.ajax({
            url: "/NCDangKyXetTuyens/KhoiNganhListAll",
            type: "GET",
            contentType: "application/json;charset=utf-8",
            dataType: "json",
            success: function (result_all) {
                $.each(result_all, function (i, result) {
                    $('#select_khoinganh').append('<option value="' + result.khoiNganh_ID + '">' + result.khoiNganh_Ten + '</option>');
                });
                $('#select_khoinganh').val(khoinganh_key);
                get_data_forselect_nganh_all(khoinganh_key, nganh_key, tohopmon_key);
            },
            error: function (errormessage) {
                alert(errormessage.responseText);
            }
        });
    }
    function get_data_forselect_nganh_all(id, nganh_key, tohopmon_key) { // id = KhoiNganh_ID
        $("#select_nganh").empty();
        $.ajax({
            url: "/NCDangKyXetTuyens/NganhListAll/" + id,
            type: 'GET',
            contentType: 'application/json;charset=utf-8',
            dataType: 'json',
            success: function (result_all) {

                $.each(result_all, function (i, result) {
                    $('#select_nganh').append('<option value="' + result.nganh_ID + '">' + result.nganh_GhiChu + '</option>');
                });

                $('#select_nganh').val(nganh_key);
                get_data_forselect_tohopmon_all(nganh_key, tohopmon_key);
            },
            error: function (errormessage) {
                alert(errormessage.responseText);
            }
        });
    }
    function get_data_forselect_tohopmon_all(id, tohopmon_key) { // id = Nganh_ID
        console.log(id + "-" + tohopmon_key)
        /* return;*/
        $("#select_tohopmon").empty();
        $.ajax({
            url: "/NCDangKyXetTuyens/ToHopMonNganhListAll/" + id,
            type: 'GET',
            contentType: 'application/json;charset=utf-8',
            dataType: 'json',
            success: function (result_all) {
                $.each(result_all, function (i, result) {
                    $('#select_tohopmon').append('<option value="' + result.thm_ID + '">' + result.thm_MaTen + '</option>');
                });
                $('#select_tohopmon').val(tohopmon_key);
            },
            error: function (errormessage) {
                alert(errormessage.responseText);
            }
        });
    }

