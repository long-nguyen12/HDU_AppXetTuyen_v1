        $('#datepicker').val('');
        $('#files_hocba').val(null);
        $('#FilesList_HocBa_View tbody').empty();
        $('#files_cccd').val(null);
        $('#FilesList_CCCD_View tbody').empty();
        $('#files_bangtn').val(null);
        $('#FilesList_BangTN_View tbody').empty();
        $('#files_chungnhan').val(null);
        $('#FilesList_ChungNhan_View tbody').empty();
        $('#files_uutien').val(null);
        $('#FilesList_UuTien_View tbody').empty();
        check_mc_chungnhan = 0;
        $('#File_ChungNhan_Error').text('');
        $('.text-danger').text('');

        HocBaFormData = new FormData();
        CCCDFormData = new FormData();
        BangTNFormData = new FormData();
        ChungNhanFormData = new FormData();
        UuTienFormData = new FormData();
        check_mc_chungnhan = 0;

        fileData = [
            { data: HocBaFormData, name: "HOCBA", savePath: "" },
            { data: CCCDFormData, name: "CCCD", savePath: "" },
            { data: BangTNFormData, name: "BANGTN", savePath: "" },
            { data: ChungNhanFormData, name: "CHUNGNHAN", savePath: "" },
            { data: UuTienFormData, name: "UUTIEN", savePath: "" },
        ];
    }


    function UpdateRecordOnclick(Dkxt_ID, Check_tt) {
        if (Check_tt != 9) {
            let str_url = '?dkxt_id=' + Dkxt_ID;
            window.location.href = "/DangKyXetTuyenKhac/IeltsEdit" + str_url;
        }
        else {
            $("#sp_noidung_thongbao_xoa").css("color", "blue");
            $('#sp_noidung_thongbao_xoa').text('Nguyện vọng đã được duyệt, không thể sửa!');
            $('#bt_dongy_on_modal_xoa').hide();
            $('#modal_tb_xoa').modal('show');
        }
    }
    function GetAllKhoiNganh() {
        $.ajax({
            url: "/DangKyXetTuyenThang/GetAllKhoiNganh/",
            type: 'GET',
            contentType: 'application/json;charset=utf-8',
            dataType: 'json',
            success: function (response) {
                let data = response;
                let khoiNganhs = $('#select_khoinganh');
                khoiNganhs.empty();
                $.each(data, function (index, item) {
                    khoiNganhs.append($('<option></option>').val(item.KhoiNganh_ID).text(item.KhoiNganh_Ten));
                });
                let id = $("#select_khoinganh").val();
                GetNganhData(id);
            },
            error: function (errormessage) {
                console.log(errormessage)
            }
        });
    }
    function GetNganhData(id) { // id = KhoiNganh_ID
        $('#Nganh_ID').empty();
        $.ajax({
            url: "/DangKyXetTuyenThang/GetAllNganhs/",
            type: 'POST',
            data: JSON.stringify({
                idKhoiNganh: id
            }),
            contentType: 'application/json;charset=utf-8',
            dataType: 'json',
            success: function (response) {
                if (response.success) {
                    let data = response.data
                    $.each(data, function (i, result) {
                        $('#Nganh_ID').append('<option value="' + result.Nganh_ID + '">' + result.NganhTenNganh + '</option>');
                    });
                    let id = $("#Nganh_ID").val();
                    GetAllChungChi(id);
                }
            },
            error: function (errormessage) {
                console.log(errormessage)
            }
        });
    }
    function GetAllChungChi(id_nganh) { // id ngành
        if (id_nganh == 0) {
            var chungchis = $('#ChungChi_ID');
            chungchis.empty();
            chungchis.append($('<option></option>').val(0).text("Chọn kết quả xét..."));
            $('#Dkxt_TongDiem').val("");
        }
        else if (id_nganh > 0) {
            $.ajax({
                url: "/DangKyXetTuyenKhac/IeltsGetAllChungChis/",
                type: 'GET',
                contentType: 'application/json;charset=utf-8',
                dataType: 'json',
                success: function (response) {
                    let chungchi = response.chungchi;
                    var chungchis = $('#ChungChi_ID');
                    chungchis.empty();
                    chungchis.append($('<option></option>').val(0).text("Chọn kết quả xét..."));
                    $.each(chungchi, function (index, item) {
                        chungchis.append($('<option></option>').val(item.ChungChi_ID).text(item.ChungChi_Ten));
                    });
                },
