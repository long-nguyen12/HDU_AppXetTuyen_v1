    function SubmitData() {
        $(".text-danger").text("");
        $('#noidung_chuyenkhoan').text('');

        let flag = true;
        let ChungChi_ID = $('#ChungChi_ID').val();
        let Nganh_ID = $('#Nganh_ID').val();
        let Dkxt_ToHopXT = $('#Dkxt_ToHopXT').val();
        let Dkxt_DonViToChuc = $('#Dkxt_DonViToChuc').val();
        let Dkxt_KetQuaDatDuoc = $('#Dkxt_KetQuaDatDuoc').val();
        let Dkxt_TongDiem = $('#Dkxt_TongDiem').val();
        let Dkxt_NgayDuThi = $('#datepicker').val();
        if (!Dkxt_DonViToChuc) {
            var errorMessage = "Vui lòng nhập đơn vị tổ chức thi";
            $('#Dkxt_DonViToChuc_Error').text(errorMessage);
            flag = false;
        }
        if ($('#select_khoinganh').val() == 0) {
            var errorMessage = "Vui lòng chọn khối ngành";
            $('#Select_KhoiNganh_Error').text(errorMessage);
            flag = false;
        }
        if (Nganh_ID == 0) {
            var errorMessage = "Vui lòng chọn ngành";
            $('#Nganh_ID_Error').text(errorMessage);
            flag = false;
        }
        if (ChungChi_ID == 0) {
            var errorMessage = "Vui lòng chọn loại kết quả";
            $('#ChungChi_ID_Error').text(errorMessage);
            flag = false;
        }
        if (!Dkxt_KetQuaDatDuoc) {
            var errorMessage = "Vui lòng nhập kết quả thi";
            $('#Dkxt_KetQuaDatDuoc_Error').text(errorMessage);
            flag = false;
        } else if (parseFloat(Dkxt_KetQuaDatDuoc) < 0) {
            var errorMessage = "Kết quả không hợp lệ";
            $('#Dkxt_KetQuaDatDuoc_Error').text(errorMessage);
            flag = false;
        }
        if (parseFloat(Dkxt_KetQuaDatDuoc) > parseFloat(Dkxt_TongDiem)) {
            var errorMessage = "Kết quả không hợp lệ";
            $('#Dkxt_KetQuaDatDuoc_Error').text(errorMessage);
            flag = false;
        }
        if (!Dkxt_NgayDuThi) {
            var errorMessage = "Vui lòng nhập ngày dự thi";
            $('#Dkxt_NgayDuThi_Error').text(errorMessage);
            flag = false;
        } else if (!Dkxt_NgayDuThi.trim()) {
            var errorMessage = "Vui lòng nhập ngày dự thi";
            $('#Dkxt_KetQuaDatDuoc_Error').text(errorMessage);
            flag = false;
        }
        if (check_mc_chungnhan == 0) { $('#File_ChungNhan_Error').text('Chưa chọn tệp chứng nhận kết quả'); flag = false; }

        if (!flag) return;


        $("#loadingModal").modal("show");

        uploadFiles();
        let formData = {
            ChungChi_ID: ChungChi_ID,
            Nganh_ID: Nganh_ID,
            Dkxt_DonViToChuc: Dkxt_DonViToChuc,
            Dkxt_KetQuaDatDuoc: Dkxt_KetQuaDatDuoc,
            Dkxt_TongDiem: Dkxt_TongDiem,
            Dkxt_NgayDuThi: Dkxt_NgayDuThi,
            Dkxt_ToHopXT: Dkxt_ToHopXT,
            Dkxt_MinhChung_HB: fileData[0].savePath,
            Dkxt_MinhChung_CCCD: fileData[1].savePath,
            Dkxt_MinhChung_Bang: fileData[2].savePath,
            Dkxt_MinhChung_KetQua: fileData[3].savePath,
            Dkxt_MinhChung_UuTien: fileData[4].savePath,
        }
        console.log(formData);

        $.ajax({
            url: '/DangKyXetTuyenKhac/IeltsAdd',
            data: JSON.stringify(formData),
            type: 'POST',
            contentType: 'application/json;charset=utf-8',
            dataType: 'json',
            success: function (res) {
                if (res.success) {
                    // showToast("Thông báo", "Thêm mới nguyện vọng thành công!", "bg-success");
                    LoadData_All_Dkxt_CCNN();
                    $('#div_list_nguyenvong').css('display', 'inline');
                    $('#div_update_dk_nguyenvong').css('display', 'none');

                    $("#loadingModal").modal("hide");
                    thongbao_dangky_thanhcong(res.data);
                    onResetFields();
                } else {
                    showToast("Thông báo", "Có lỗi xảy ra khi thêm mới nguyện vọng!", "bg-error");
                    $("#loadingModal").modal("hide");
                }
            },
            error: function (errormessage) {
                showToast("Thông báo", "Có lỗi xảy ra khi thêm mới nguyện vọng!", "bg-error");
                $("#loadingModal").modal("hide");
            }
        });
    }



