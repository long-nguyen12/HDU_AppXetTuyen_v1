    function upData(value, ToHop) {
        var obj = { idDkxt: value, MaToHop: ToHop };
        $.ajax({
            url: '/DangKyXetTuyenKhac/upData',
            type: 'POST',
            dataType: 'json',
            data: JSON.stringify(obj),
            contentType: 'application/json; charset=utf-8'
        }).done(function (response) {
            if (response.status == true) {
                showToast("Thông báo", "Thay đổi thứ tự nguyện vọng thành công!", "bg-success");
                LoadData_All_Dkxt_CCNN();
                $('#modal_tb_thaydoinv').modal('hide');
