
@{
    ViewBag.Title = "Đăng ký xét tuyển";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style>
    label {
        white-space: nowrap !important;
    }

    .span_with_for {
        width: 65px !important
    }

    .table-container {
        overflow-x: auto;
        -webkit-overflow-scrolling: touch;
    }

    .btn, .form-select, .form-control {
        border-radius: 3px;
    }
</style>
<header class="page-header page-header-compact page-header-light border-bottom bg-white mb-2">
    <div class="container-fluid px-4">
        <div class="page-header-content">
            <div class="row align-items-center justify-content-between pt-3">
                <div class="col-auto mb-1">
                    <div class="page-header-title mb-2">
                        <div class="page-header-icon"><i data-feather="list"></i></div>
                        <span id="sp_tieude"></span>

                    </div>
                </div>
            </div>
        </div>
    </div>
</header>
<div class="container-fluid px-12">
    <div id="div_list_nguyenvong">
        <div class="card card-header-actions">
            <div class="card-header">
                Các nguyện vọng đã đăng ký
            </div>
            <div class="card-body">
                <div class="row">
                    <div class="col-lg-12" id="div_thead" style="display:none">
                        <div class="row g-1">
                            <div class="col-lg-1 col-sm-12"><b>Nguyện vọng</b></div>
                            <div class="col-lg-2 col-sm-12"><b>Ngành</b></div>
                            <div class="col-lg-1 col-sm-12"><b>Tên kỳ thi</b></div>
                            <div class="col-lg-2 col-sm-12"><b>Đơn vị tổ chức thi</b></div>
                            <div class="col-lg-3 col-sm-12"><b style="margin-left:10px">Kết quả thi/ Ngày thi</b></div>
                            <div class="col-lg-1 col-sm-12"><b>Ngày đăng ký</b></div>
                            <div class="col-lg-2 col-sm-12"><b style="margin-left:10px">Cập nhật</b> </div>
                        </div>
                        <div class="col-12"><hr class="mt-2 mb-0" /><hr class="mt-0 mb-2" /></div>
                    </div>
                    <div id="div_tbody_list">
                        <script>
                            $(document).ready(function () {
                                LoadData_All_Dkxt_CCNN();
                            })
                            function ConverDateToString(date) {
                                var parts = date.split('-');
                                var mydate = parts[2] + '/' + parts[1] + '/' + parts[0];
                                return mydate;
                            }
                            function LoadData_All_Dkxt_CCNN() {
                                $('#div_tbody_list').empty();
                                $.ajax({
                                    url: "/DangKyXetTuyenKhac/IeltsGetAllNguyenVongs/",
                                    type: 'POST',
                                    contentType: 'application/json;charset=utf-8',
                                    dataType: 'json',
                                    async: false,
                                    success: function (response) {
                                        let result_all = response.data;
                                        let thiSinh = response.thisinh;
                                        if (thiSinh) {
                                            let hocLuc = thiSinh.ThiSinh_HocLucLop12;
                                            let hanhKiem = thiSinh.ThiSinh_HanhKiemLop12;
                                            let hocLucName = "";
                                            let hanhKiemName = "";
                                            if (hocLuc == 1) hocLucName = "Trung bình";
                                            else if (hocLuc == 2) hocLucName = "Khá";
                                            else if (hocLuc == 3) hocLucName = "Giỏi";
                                            else if (hocLuc == 4) hocLucName = "Xuất sắc";
                                            $('#Dkxt_XepLoaiHocLuc_12').val(hocLucName);

                                            if (hanhKiem == 1) hanhKiemName = "Yếu";
                                            else if (hanhKiem == 2) hanhKiemName = "Trung bình";
                                            else if (hanhKiem == 3) hanhKiemName = "Khá";
                                            else if (hanhKiem == 4) hanhKiemName = "Tốt";
                                            $('#Dkxt_XepLoaiHanhKiem_12').val(hanhKiemName);

                                            $('#span_Uutien_khuvuc').text(thiSinh.ThiSinh_UTKV);
                                            $('#span_Uutien_doituong').text(thiSinh.ThiSinh_UTDT);

                                        }
                                        if (!result_all || result_all.length == 0) {
                                            let div_item = ` <div class="col-12 text-center">Chưa có dữ liệu đăng ký xét tuyển</div>`;
                                            $('#div_tbody_list').append(div_item);
                                        }

                                        else {
                                            $.each(result_all, function (i, result) {
                                                if (result_all.length == 1) {
                                                    let div_item = "";
                                                    div_item += `<div class="row g-1">`
                                                    div_item += `<div class="col-lg-1 col-sm-12 text-nowrap"><span class="col-4 span_thongtin_nv">- </span> Nguyện vọng ` + result.Dkxt_NguyenVong + `</div>`;
                                                    div_item += `<div class="col-lg-2 col-sm-12 text-nowrap"><span class="col-4 span_thongtin_nv">- Ngành: </span><span class="col-8 span_noidung_nv ">` + result.Nganh_All.Nganh_GhiChu + `</span></div>`;
                                                    div_item += `<div class="col-lg-1 col-sm-12 text-nowrap"><span class="col-4 span_thongtin_nv">- Tên kỳ thi: </span> <span class="col-8 span_noidung_nv">` + result.Dkxt_ChungChi_Ten.LoaiCC + `</span></div>`;
                                                    div_item += `<div class="col-lg-2 col-sm-12 text-nowrap"><span class="col-4 span_thongtin_nv">- Đơn vị tổ chức: </span> <span class="col-8 span_noidung_nv">` + result.Dkxt_DonViToChuc + `</span> <b></b></div>`;
                                                    div_item += `<div class="col-lg-3 col-sm-12 text-nowrap"><span class="col-5 span_thongtin_nv">- Kết quả thi/ ngày thi: </span><br class="span_thongtin_nv"/><span class="col-7 span_noidung_nv" style="margin:10px">` + result.Dkxt_KetQuaDatDuoc.toFixed(2) + "/" + result.Dkxt_TongDiem.toFixed(2) + `(điểm)</span><span>(ngày: ` + result.Dkxt_NgayDuThi + `)</span></div>`;
                                                    div_item += `<div class="col-xl-1 col-lg-12  text-nowrap"><span class="col-4  span_thongtin_nv">- Ngày đăng ký: </span><span class="col-8 span_noidung_nv">` + ConverDateToString(result.Dkxt_NgayDangKy) + `</span></div>`;
                                                    div_item += `<div class="col-lg-2 col-sm-12 text-nowrap">`
                                                    div_item += `<button class="btn btn-outline-success  btn-sm" onclick="DkxtCCnnDetail(${result.Dkxt_ID})">Xem</button>&nbsp;<button class="btn btn-outline-primary  btn-sm" onclick="UpdateRecordOnclick(${result.Dkxt_ID},${result.Dkxt_TrangThai_HoSo})">Sửa</button>&nbsp;<button class="btn btn-outline-danger btn-sm" onclick="DeleteRecordOnclick(${result.Dkxt_ID},${result.Dkxt_TrangThai_HoSo},'${result.Dkxt_ToHopXT}')">Xóa</button></div>`;
                                                    div_item += `</div>`
                                                    div_item += `<div class="col-12" > <hr class="mt-2 mb-2" /></div>`;
                                                    $('#div_tbody_list').append(div_item);
                                                }
                                                else if (result_all.length > 1) {
                                                    let div_item = "";
                                                    div_item += `<div class="row g-1">`
                                                    div_item += `<div class="col-lg-1 col-sm-12 text-nowrap"><span class="col-4 span_thongtin_nv">- </span> Nguyện vọng ` + result.Dkxt_NguyenVong + `</div>`;
                                                    div_item += `<div class="col-lg-2 col-sm-12 text-nowrap"><span class="col-4 span_thongtin_nv">- Ngành: </span><span class="col-8 span_noidung_nv text-wrap">` + result.Nganh_All.Nganh_GhiChu + `</span></div>`;
                                                    div_item += `<div class="col-lg-1 col-sm-12 text-nowrap"><span class="col-4 span_thongtin_nv">- Tên kỳ thi: </span> <span class="col-8 span_noidung_nv">` + result.Dkxt_ChungChi_Ten.LoaiCC + `</span></div>`;
                                                    div_item += `<div class="col-lg-2 col-sm-12 text-nowrap"><span class="col-4 span_thongtin_nv">- Đơn vị tổ chức: </span> <span class="col-8 span_noidung_nv">` + result.Dkxt_DonViToChuc + `</span> <b></b></div>`;
                                                    div_item += `<div class="col-lg-3 col-sm-12 text-nowrap"><span class="col-5 span_thongtin_nv">- Kết quả thi/ ngày thi: </span><br class="span_thongtin_nv"/><span class="col-7 span_noidung_nv" style="margin:10px">` + result.Dkxt_KetQuaDatDuoc.toFixed(2) + "/" + result.Dkxt_TongDiem.toFixed(2) + ` (điểm)</span><span>(ngày: ` + result.Dkxt_NgayDuThi + `)</span></div>`;
                                                    div_item += `<div class="col-xl-1 col-lg-12  text-nowrap"><span class="col-4  span_thongtin_nv">- Ngày đăng ký: </span><span class="col-8 span_noidung_nv">` + ConverDateToString(result.Dkxt_NgayDangKy) + `</span></div>`;
                                                    div_item += `<div class="col-lg-2 col-sm-12 text-nowrap">`
                                                    div_item += `<button class="btn btn-outline-info  btn-sm" style="margin-left:10px"  onclick = "ShowModalChangeData(${result.Dkxt_ID}, '${result.Dkxt_ToHopXT}',1)" ${result.Dkxt_NguyenVong == 1 ? "disabled" : ''}><b>&#8743;</b></button>&nbsp;<button class="btn btn-outline-warning  btn-sm" onclick="ShowModalChangeData(${result.Dkxt_ID}, '${result.Dkxt_ToHopXT}',2)" ${i == result_all.length - 1 ? "disabled" : ""}><b>&#8744;</b></button>&nbsp;`;
                                                    div_item += `<button class="btn btn-outline-success  btn-sm" onclick="DkxtCCnnDetail(${result.Dkxt_ID})">Xem</button>&nbsp;<button class="btn btn-outline-primary  btn-sm" onclick="UpdateRecordOnclick(${result.Dkxt_ID},${result.Dkxt_TrangThai_HoSo})">Sửa</button>&nbsp;<button class="btn btn-outline-danger btn-sm" onclick="DeleteRecordOnclick(${result.Dkxt_ID},${result.Dkxt_TrangThai_HoSo},'${result.Dkxt_ToHopXT}')">Xóa</button></div>`;
                                                    div_item += `</div>`
                                                    div_item += `<div class="col-12" ><hr class="mt-2 mb-2" /></div>`;
                                                    $('#div_tbody_list').append(div_item);
                                                }
                                            });
                                        }
                                        if (window.innerWidth <= 1000) {

                                            $('#div_thead').css('display', 'none');
                                            $('#div_thead').hide();
                                            $('.span_thongtin_nv').css('display', 'inline');
                                            $('.span_thongtin_nv').show();
                                            $('.span_noidung_nv').css('font-weight', 'bold')
                                        }
                                        else {

                                            $('#div_thead').css('display', 'inline');
                                            $('#div_thead').show();
                                            $('.span_thongtin_nv').css('display', 'none');
                                            $('.span_thongtin_nv').hide();
                                            $('.span_noidung_nv').css('font-weight', 'normal')
                                        }
                                    },
                                    error: function (errormessage) {
                                        console.log(errormessage)
                                    }
                                });
                            }
                            function DkxtCCnnDetail(dkxt_id) {
                                var str_url = '?dkxt_id=' + dkxt_id;
                                window.location.href = "/DangKyXetTuyenKhac/IeltsDetail" + str_url;
                            }
                        </script>
                    </div>
                </div>

                <div class="row mt-2">
                    <div class="col-xxl-12">
                        <center>
                            <button id="bt_xuat_phieu_tong_hop_kq" class="btn btn-outline-success ">Xuất phiếu tổng hợp</button>
                            <a id="bt_add_aspirations" class="btn btn-outline-primary" onclick="bt_Insert_Dknv_on_divView_Click(0,0,0)">Thêm nguyện vọng</a>
                            <script>
                                $(document).ready(function () {
                                    let sp_tieude = $('#sp_tieude');
                                    let bt_xuat_phieu_tong_hop = $('#bt_xuat_phieu_tong_hop_kq');
                                    let bt_them_nguyen_vong = $('#bt_add_aspirations');
                                    if (window.innerWidth <= 1000) {

                                        sp_tieude.text('ĐKXT SD chứng chỉ ngoại ngữ');
                                        bt_xuat_phieu_tong_hop.text('Xuất phiếu');
                                        bt_them_nguyen_vong.text("Thêm nguyện vọng");

                                        $('#div_thead').css('display', 'none');
                                        $('#div_thead').hide();
                                        $('.span_thongtin_nv').css('display', 'inline');
                                        $('.span_thongtin_nv').show();
                                        $('.span_noidung_nv').css('font-weight', 'bold')
                                    }
                                    else {
                                        sp_tieude.text('Đăng ký xét tuyển sử dụng chứng chỉ ngoại ngữ');
                                        bt_xuat_phieu_tong_hop.text('Xuất phiếu tổng hợp');
                                        bt_them_nguyen_vong.text("Thêm nguyện vọng");

                                        $('#div_thead').css('display', 'inline');
                                        $('#div_thead').show();
                                        $('.span_thongtin_nv').css('display', 'none');
                                        $('.span_thongtin_nv').hide();
                                        $('.span_noidung_nv').css('font-weight', 'normal')
                                    }
                                    function reportWindowSize() {
                                        if (window.innerWidth <= 1000) {

                                            sp_tieude.text('ĐKXT SD chứng chỉ ngoại ngữ');
                                            bt_xuat_phieu_tong_hop.text('Xuất phiếu');
                                            bt_them_nguyen_vong.text("Thêm nguyện vọng");

                                            $('#div_thead').css('display', 'none');
                                            $('#div_thead').hide();
                                            $('.span_thongtin_nv').css('display', 'inline');
                                            $('.span_thongtin_nv').show();
                                            $('.span_noidung_nv').css('font-weight', 'bold')

                                        }
                                        else {
                                            sp_tieude.text('Đăng ký xét tuyển sử dụng chứng chỉ ngoại ngữ');
                                            bt_xuat_phieu_tong_hop.text('Xuất phiếu tổng hợp');
                                            bt_them_nguyen_vong.text("Thêm nguyện vọng");

                                            $('#div_thead').css('display', 'inline');
                                            $('#div_thead').show();
                                            $('.span_thongtin_nv').css('display', 'none');
                                            $('.span_thongtin_nv').hide();
                                            $('.span_noidung_nv').css('font-weight', 'normal')
                                        }
                                    }
                                    window.onresize = reportWindowSize;
                                })
                            </script>
                        </center>
                    </div>
                </div>
                <div class="row">
                    <div class="col-12"><hr class="mt-2 mb-0" /></div>
                </div>
                <div class="row">
                    <div class="col-xxl-12">
                        <p style="color:red; font-style:italic" class="mb-1">Lưu ý: </p>
                        <p style="color: red; font-style: italic; text-align: justify " class="mb-1">- Thí sinh cần nộp lệ phí xét tuyển và gửi minh chứng (ảnh chụp giao dịch chuyển tiền).</p>
                        <p style="color: red; font-style: italic; text-align: justify" class="mb-1">- Thí sinh in phiếu tổng hợp nguyện vọng đăng ký, kiểm tra lại thông tin trong phiếu, ký xác nhận và gửi về trường theo đường bưu điện.</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div id="div_update_dk_nguyenvong" style="display: none">
        <div class="card card-header-actions">
            <div class="card-header">
                Đăng ký nguyện vọng mới
                <button class="btn-close" type="button" aria-label="Close" onclick="_bt_HuyBo_Dkxt_NVM_OnClick(0,0,0)"></button>
            </div>
            <div class="card-body">
                <div class="row g-3">
                    <div class="col-xl-4 col-md-12">
                        <div class="row">
                            <div class="col-12"><b>Chọn ngành và loại kết quả xét <font color="red">(*)</font></b></div>
                            <div class="col-12"><hr class="mt-1 mb-0" /><hr class="mt-0 mb-2" /></div>
                        </div>
                        <div class="row">
                            <div class="col-4">Khối ngành</div>
                            <div class="col-8">
                                <select id="select_khoinganh" class="form-select"></select>
                                <span id="Select_KhoiNganh_Error" class="small mb-1 text-danger"></span>
                            </div>
                        </div>
                        <div class="col-12"><hr class="mt-1 mb-2" /></div>
                        <div class="row">
                            <div class="col-4">Ngành</div>
                            <div class="col-8">
                                <select id="Nganh_ID" class="form-select"></select>
                                <span id="Nganh_ID_Error" class="small mb-1 text-danger"></span>
                            </div>
                        </div>
                        <div class="col-12"><hr class="mt-1 mb-2" /></div>
                        <div class="row">
                            <div class="col-4 ">Chứng chỉ</div>
                            <div class="col-8">
                                <select id="ChungChi_ID" class="form-select"></select>
                                <span id="ChungChi_ID_Error" class="small mb-1 text-danger"></span>
                                <input id="Dkxt_ToHopXT" type="hidden" class="form-control" value="" disabled />
                            </div>
                            <div class="col-12"><hr class="mt-1 mb-2" /></div>
                        </div>
                        <div class="row" style="margin-top:15px">
                            <div class="col-6 ">
                                <span id="span_Uutien_doituong"></span>
                                <hr class="mt-2 md-2" />
                            </div>
                            <div class="col-6  ">
                                <span id="span_Uutien_khuvuc"></span>
                                <hr class="mt-2 md-2" />
                            </div>
                        </div>
                        <div class="row mb-1">
                            <div class="col-6">
                                <span> Học lực lớp 12</span>
                                <input id="Dkxt_XepLoaiHocLuc_12" type="text" class="form-control" value="" disabled />
                                <span id="Dkxt_XepLoaiHocLuc_12_Error" class="small mb-1 text-danger"></span>
                            </div>
                            <div class="col-6">
                                <span>Hạnh kiểm lớp 12</span>
                                <input id="Dkxt_XepLoaiHanhKiem_12" type="text" class="form-control" value="" disabled />
                                <span id="Dkxt_XepLoaiHanhKiem_12_Error" class="small mb-1 text-danger"></span>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-4 col-md-12">
                        <div class="row">
                            <div class="col-12"><b>Thông tin kết quả nộp xét tuyển <font color="red">(*)</font></b></div>
                        </div>
                        <div class="row"><div class="col-12"><hr class="mt-1 mb-0" /><hr class="mt-0 mb-2" /></div></div>
                        <div class="row">
                            <div class="col-4">Tên đơn vị</div>
                            <div class="col-8">
                                <input id="Dkxt_DonViToChuc" type="text" class="form-control" placeholder="Tên đơn vị tổ chức thi" value="" />
                                <span id="Dkxt_DonViToChuc_Error" class="small mb-1 text-danger"></span>
                            </div>
                        </div>
                        <div class="row"><div class="col-12"><hr class="mt-1 mb-2" /></div></div>
                        <div class="row">
                            <div class="col-4">Thang điểm</div>
                            <div class="col-8">
                                <input id="Dkxt_TongDiem" type="number" class="form-control" placeholder="Thang điểm" value="" disabled />
                                <span id="Dkxt_TongDiem_Error" class="small mb-1 text-danger"></span>
                            </div>
                        </div>
                        <div class="row"><div class="col-12"><hr class="mt-1 mb-2" /></div></div>
                        <div class="row">
                            <div class="col-4">Kết quả thi</div>
                            <div class="col-8">
                                <input id="Dkxt_KetQuaDatDuoc" type="number" class="form-control" placeholder="Kết quả" value="" />
                                <span id="Dkxt_KetQuaDatDuoc_Error" class="small mb-1 text-danger"></span>
                            </div>
                        </div>
                        <div class="row"><div class="col-12"><hr class="mt-1 mb-2" /></div></div>
                        <div class="row">
                            <div class="col-4">Ngày thi</div>
                            <div class="col-8">
                                <input id="datepicker" name="Dkxt_NgayDuThi" type="text" class="form-control" placeholder="Ngày thi" value="" onchange="RemoveErrorDate()" />
                                <span id="Dkxt_NgayDuThi_Error" class="small mb-1 text-danger"></span>
                                <script>function RemoveErrorDate() { $('#Dkxt_NgayDuThi_Error').text(''); }</script>
                            </div>
                        </div>
                        <div class="row mt-0">
                            <div class="col-12" style="text-align:center">
                                <span id="Diem_All_Error" class="small mb-1 text-danger"></span>
                            </div>
                        </div>
                        <div class="row"><div class="col-12"><hr class="mt-1 mb-2" /></div></div>

                        <div class="row mb-0">
                            <div class="col-12"><b>Chứng nhận kết quả thi <font color="red">(*)</font></b></div>
                            <div class="col-12">
                                <input class="form-control small" id="files_chungnhan" multiple="multiple" name="files_chungnhan" type="file" data-val="true" accept="application/pdf,.png, .jpeg, .jpg" />
                                <span id="File_ChungNhan_Error" class="small mb-1 text-danger"></span>
                            </div>
                            <div class="col-md-12 mb-0">
                                <table class="table mb-0" id="FilesList_ChungNhan_View" style="visibility:hidden; width: 100%">
                                    <tr style="display:none"> <th colspan="2"><input type="hidden" id="ChungNhan_Files" /></th></tr>
                                </table>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-4 col-md-12">
                        <div class="row">
                            <div class="col-12"><b>Tệp minh chứng khác</b></div>
                        </div>
                        <div class="row"><div class="col-12"><hr class="mt-1 mb-0" /><hr class="mt-0 mb-2" /></div></div>
                        <div class="row">
                            <div class="col-12">Học bạ</div>
                            <div class="col-12">
                                <input class="form-control small" id="files_hocba" multiple="multiple" name="files_hocba" type="file" data-val="true" accept="application/pdf,.png, .jpeg, .jpg" />
                            </div>
                            <div class="col-12">
                                <table class="table" id="FilesList_HocBa_View" style="visibility:hidden; width: 100%">
                                    <tr style="display:none"><th colspan="2"><input type="hidden" id="HocBa_Files" /></th></tr>
                                </table>
                            </div>
                        </div>
                        <div class="row"><div class="col-12"><hr class="mt-0 mb-2" /></div></div>
                        <div class="row">
                            <div class="col-12">Căn cước công dân</div>
                            <div class="col-12"><input class="form-control small" id="files_cccd" multiple="multiple" name="files_cccd" type="file" data-val="true" accept="application/pdf,.png, .jpeg, .jpg" /></div>
