<script>
    $(document).ready(function () {
        let sp_tieude = $('#sp_tieude');
        if (window.innerWidth <= 1200) {
            sp_tieude.text("Đăng ký tuyển thẳng");
            $('#files_chungnhan').hide();
            $('#files_hocba').hide();
            $('#files_cccd').hide();
            $('#files_bangtn').hide();
            $('#files_uutien').hide();
        }
        else {
            sp_tieude.text("Đăng ký tuyển thẳng theo quy định của trường (áp dụng cho học sinh đạt giải các môn học từ giải Ba trở lên)");
            $('#files_chungnhan').show();
            $('#files_hocba').show();
            $('#files_cccd').show();
            $('#files_bangtn').show();
            $('#files_uutien').show();
        }
        function reportWindowSize() {
            if (window.innerWidth <= 1200) {
                sp_tieude.text("Đăng ký tuyển thẳng");
                $('#files_chungnhan').hide();
                $('#files_hocba').hide();
                $('#files_cccd').hide();
                $('#files_bangtn').hide();
                $('#files_uutien').hide();
            }
            else {
                sp_tieude.text("Đăng ký tuyển thẳng theo quy định của trường (áp dụng cho học sinh đạt giải các môn học từ giải Ba trở lên)");
                $('#files_chungnhan').show();
                $('#files_hocba').show();
                $('#files_cccd').show();
                $('#files_bangtn').show();
                $('#files_uutien').show();
            }
        }
        window.onresize = reportWindowSize;
    })
</script>
